{% extends 'base.html.twig' %}

{% block layout %}

    <div class="container-fluid">

        <div class="row">
            <div class="col p-0">
                {% include 'components/_header.html.twig' %}
            </div>
        </div>

        <div class="row">
            <div class="col col-2">
                {% block sidebarLeft %}{% endblock %}
            </div>

            <div class="col bg-light">
                {% block content %}{% endblock %}
            </div>

            <div class="col col-3">
                {% block sidebarRight %}{% endblock %}
            </div>
        </div>

    </div>
{% endblock %}
